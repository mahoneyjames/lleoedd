{
  "name": "lleoedd",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "now": {
    "dotenv": "variables.env.now"
  },
  "scripts": {
    "aws": "node ./app.js",
    "start": "node ./app.js",
    "watch": "nodemon ./app.js --ignore public/",
    "dev": "concurrently \"npm run watch\" \"npm run assets\" --names \"ðŸ’»,ðŸ“¦\" --prefix name",
    "debug": "node ./app.js --inspect",
    "mongo": "node ./data/mongoplay.js",
    "assets": "webpack -w --display-max-modules 0",
    "sample": "node ./data/load-sample-data.js",
    "list": "node ./data/list.js",
    "cleanplaces": "node ./data/load-sample-data.js --delete",
    "migrate": "node ./data/migration.js",
    "now": "now -e DB_USER=@db_user -e DB_PASS=@db_pass -e NODE_ENV=\"production\" -e PORT=80",
    "labels": "node ./setup.js"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "aws-sdk": ">= 2.0.9",
    "axios": "0.15.3",
